<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Daycare_Results"
    >

    <ImageView
        android:id="@+id/image_View7"
        android:layout_width="412dp"
        android:layout_height="96dp"
        android:contentDescription="@string/book_search_bar2"
        android:src="@drawable/srh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_booking2"
        app:layout_constraintVertical_bias="0.067"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/et_booking2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.002"
        app:srcCompat="@drawable/daycarebook"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/tv_search"
        android:layout_width="386dp"
        android:layout_height="55dp"
        android:text="@string/tv_search"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_View7"
        app:layout_constraintVertical_bias="0.0" />

    <Switch
        android:id="@+id/switch_location"
        android:layout_width="384dp"
        android:layout_height="36dp"
        android:background="#dee1ec"
        android:text="@string/switch_location"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.342"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_search"
        app:layout_constraintVertical_bias="0.033"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="428dp"
        android:layout_height="220dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="1dp"
        android:onClick="gotoshowmore"
        android:contentDescription="@string/pet_description1"
        app:layout_constraintBottom_toTopOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch_location"
        app:layout_constraintVertical_bias="0.19"
        app:srcCompat="@drawable/petdetails1" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="448dp"
        android:layout_height="257dp"
        android:contentDescription="@string/pet_description2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch_location"
        app:layout_constraintVertical_bias="0.645"
        app:srcCompat="@drawable/petdetails2" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="295dp"
        android:layout_height="88dp"
        android:onClick="gotofavpage2"
        android:contentDescription="@string/watch_favourite_list2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.809"
        app:srcCompat="@drawable/favlist1" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="230dp"
        android:layout_height="54dp"
        android:layout_marginTop="60dp"
        android:max="5"
        android:rating="3"
        android:scaleX="0.7"
        android:scaleY="0.7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch_location"
        tools:ignore="SpeakableTextPresentCheck" />

    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="235dp"
        android:layout_height="59dp"
        android:max="5"
        android:rating="4"
        android:scaleX="0.7"
        android:scaleY="0.7"
        app:layout_constraintBottom_toTopOf="@+id/imageView25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.744"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar"
        app:layout_constraintVertical_bias="0.738"
        tools:ignore="SpeakableTextPresentCheck" />

    <SearchView
        android:id="@+id/et_searchbar"
        android:layout_width="320dp"
        android:layout_height="57dp"
        android:queryHint="@string/et_searchbar"
        app:layout_constraintBottom_toBottomOf="@+id/image_View7"
        app:layout_constraintEnd_toEndOf="@+id/image_View7"
        app:layout_constraintStart_toStartOf="@+id/image_View7"
        app:layout_constraintTop_toTopOf="@+id/image_View7" />
</androidx.constraintlayout.widget.ConstraintLayout>